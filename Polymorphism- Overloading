# basics
java basics programs .
public class Overload 
{
int add (int a, int b)
{
int c= a+b;
System.out.println(c);
return c;
}
int add (int a,int b,int c)
{
int d=a+b+c;
System.out.println(d);
return d;
}
public static void main(String args[])
{
Overload obj = new Overload();
obj.add(12,10);
obj.add(10,45,10);
}
}
